{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {"ignoreComments": true, "ignoreTemplateLiterals": true, "ignoreStrings": true }],
        "import/extensions": ["error", "always", { "ignorePackages": true }]
    }
}
